      display: flex;
      justify-content: center;
      margin-bottom: 20px;
    }
.back{
  margin-right: 10px;
  margin-top: 15px;
  font-size: 20px;
}
    #search-input {
      width: 60%;
      padding: 15px;
      font-size: 16px;
      border: 2px solid #ccc;
      border-radius: 30px;
      outline: none;
      transition: all 0.3s ease;
    }

    #search-input:focus {
      border-color: #ff6347;
      box-shadow: 0 0 10px rgba(255, 99, 71, 0.3);
    }

    /* Product Grid */
    .section-p1 {
      padding: 40px 20px;
      background-color: #f5f5f5;
    }

    .pro-container {
      display: flex;
      justify-content: space-between;
      flex-wrap: wrap;
      gap: 20px;
    }

    .pro {
      background-color: white;
      border: 1px solid #ddd;
      border-radius: 8px;
      padding: 15px;
      width: 23%;
      box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);
      transition: transform 0.3s ease;
    }

    .pro:hover {
      transform: scale(1.05);
    }

    .pro img {
      width: 100%;
      border-radius: 8px;
    }

    .des {
      text-align: center;
      padding: 10px 0;
    }

    .star i {
      color: #ffb400;
      margin-bottom:10px ;
    }

    h4 {
      color: #333;
    }

    /* No products message */
    #no-products {
      text-align: center;
      font-size: 18px;
      color: #666;
      display: none;
    }
    h5{
      font-size: 12px;
      margin: 14px;
    }
    span{
font-size: 12px;
    }

    /* Responsive Design */
    @media (max-width: 768px) {
      .pro {
        width: 48%;
