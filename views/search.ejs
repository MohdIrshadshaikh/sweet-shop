  <!-- JavaScript to handle the search functionality -->
  <script>
    // On page load, hide all products and show "No products yet"
    window.onload = function () {
      const products = document.querySelectorAll(".pro");
      products.forEach(function (product) {
        product.style.display = "none"; // Hide all products
      });
      document.getElementById("no-products").style.display = "block"; // Show "No products yet" message
    };
  
    // Search functionality
    document.getElementById("search-input").addEventListener("input", function () {
      const searchQuery = this.value.toLowerCase();
      const products = document.querySelectorAll(".pro");
  
      if (searchQuery === "") {
        products.forEach(function (product) {
          product.style.display = "none"; // Hide all products
        });
        document.getElementById("no-products").style.display = "block"; // Show "No products yet" message
      } else {
        let hasResults = false;
        products.forEach(function (product) {
          const productName = product.querySelector("h5").textContent.toLowerCase();
          if (productName.includes(searchQuery)) {
            product.style.display = "block"; // Show matching products
            hasResults = true;
          } else {
            product.style.display = "none"; // Hide non-matching products
          }
        });
  
        // Toggle "No products yet" message based on results
        document.getElementById("no-products").style.display = hasResults ? "none" : "block";
      }
    });
  </script>
  
  

</body>
</html>
